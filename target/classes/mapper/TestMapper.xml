<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.quartz.mapper.TestMapper">

    <select id="selectUserInfo" resultType="User">
        select 
            id,
            name,
            age 
        from 
            user
        order by
            id desc
    </select>

</mapper>